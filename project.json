{
  "name": "sdk-flutter",
  "sourceRoot": "libs/sdk-flutter/lib",
  "projectType": "library",
  "tags": [],
  "targets": {
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter analyze",
        "cwd": "libs/sdk-flutter"
      }
    },
    "pod:lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pod lib lint calljmp.podspec",
        "cwd": "libs/sdk-flutter/ios"
      }
    },
    "publish:git": {
      "executor": "nx:run-commands",
      "dependsOn": [
        "typecheck",
        "pod:lint"
      ],
      "options": {
        "cwd": "libs/sdk-flutter",
        "commands": [
          "./tool/git-publish.sh"
        ]
      }
    },
    "publish:pubdev": {
      "executor": "nx:run-commands",
      "dependsOn": [
        "typecheck",
        "pod:lint"
      ],
      "options": {
        "cwd": "libs/sdk-flutter",
        "commands": [
          "dart pub publish"
        ],
        "parallel": false
      }
    }
  }
}